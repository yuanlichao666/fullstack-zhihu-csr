{
  "name": "vue3-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path ../../.gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "auto-config": "autoConfig"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@types/lodash-es": "^4.17.12",
    "@unocss/preset-rem-to-px": "^65.4.3",
    "@unocss/reset": "^65.4.3",
    "@vueuse/core": "^12.5.0",
    "@zh-plugin/icons": "workspace:^",
    "element-plus": "^2.9.4",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.8",
    "@eslint/eslintrc": "^3.2.0",
    "@iconify-json/carbon": "^1.2.6",
    "@iconify-json/ep": "^1.2.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.1",
    "@unocss/preset-wind": "^65.4.3",
    "@unocss/transformer-directives": "^65.4.3",
    "@unocss/transformer-variant-group": "^65.4.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "1.1.28",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@zh-plugin/tsconfig": "workspace:^",
    "cypress": "^14.0.2",
    "eslint": "^9.20.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-oxlint": "^0.15.10",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.1",
    "oxlint": "^0.15.10",
    "postcss-mobile-forever": "^4.3.2",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "prettier": "^3.5.0",
    "sass-embedded": "^1.83.4",
    "start-server-and-test": "^2.0.10",
    "typescript": "~5.7.3",
    "unocss": "^65.4.3",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-icons": "^22.0.0",
    "unplugin-vue-components": "^28.0.0",
    "unplugin-vue-macros": "^2.14.2",
    "unplugin-vue-router": "^0.11.2",
    "vite": "^6.1.0",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.1.10"
  }
}
